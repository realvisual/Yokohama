<?xml version="1.0" encoding="GBK"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <welcome-file-list>
    <welcome-file>welcome.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>com.yoko.Controller.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/admin/log.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>upload</servlet-name>
    <servlet-class>com.yoko.Controller.SmartuploadAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>upload</servlet-name>
    <url-pattern>/admin/upload</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>uploadMessage</servlet-name>
    <servlet-class>com.yoko.Controller.UploadMessage</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>uploadMessage</servlet-name>
    <url-pattern>/uploadMessage.do</url-pattern>
  </servlet-mapping>
</web-app>